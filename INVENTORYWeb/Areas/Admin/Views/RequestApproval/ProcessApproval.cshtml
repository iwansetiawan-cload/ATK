                                success: function (data) {
                                    if (data.success) {
                                        swal("Approve!", data.message, "success");
                                        $("#curent_status").val(data.status);
                                    }
                                    else {
                                        swal("Approve!", data.message, "error");
                                    }
                                    dataTable.ajax.reload();
                                }
                            });
                        });
                    }
                  
                }
            });


            
        }
