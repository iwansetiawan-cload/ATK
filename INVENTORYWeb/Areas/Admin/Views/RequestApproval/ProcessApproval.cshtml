                                success: function (data) {
                                    if (data.success) {
                                        swal("Reject!", data.message, "success");
                                        if (data.status != '') {
                                            $("#curent_status").val(data.status);
                                        }
                                    }
                                    else {
                                        swal("Reject!", data.message, "error");
                                    }
                                    // $("#rejectHeader_note").val('');
                                    dataTable.ajax.reload();
                                }
                            });
                        });
                    }
                  
                }
            });

        }

        function GetIdItem(id) {
          $("#GetIdTemp").val(id);
        }

        function SimpanDataAdjust() {
            var url = "/Admin/RequestApproval/AddAdjustItem";
            $.ajax({
                type: "POST",
                url: url,
                data: {
                    "id": $("#GetIdTemp").val(),
                    "qty": $("#GetQty").val(),
                    "notes": $("#GetNotes").val()
                },
                success: function (data) {
                    if (data.success) {
                        new PNotify({
                            title: 'Simpan Berhasil ',
                            text: data.message,
                            icon: 'icofont icofont-info-circle',
                            type: 'success'
                        });
                        $("#curent_status").val(data.status);
                        $("#GetIdTemp").val('');
                        $("#GetQty").val('');
                        $("#GetNotes").val('');                       
                        $('#lookupAdjust').modal('hide');
                        dataTable.ajax.reload();

                    } else {
                        new PNotify({
                            title: 'Error Simpan ',
                            text: data.message,
                            icon: 'icofont icofont-info-circle',
                            type: 'error'
                        });
                    }

                }
            });
        }

        
    </script>

}
